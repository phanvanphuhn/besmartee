    "@types/babel__preset-env": "^7.9.6",
    "@types/eslint": "^8.56.0",
    "@types/lodash": "^4.14.202",
    "@types/prettier": "3.0.0",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "metro-react-native-babel-preset": "^0.77.0",
