{
  "name": "besmartee",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "android": "cd android && ./gradlew clean && cd ../ && react-native run-android",
    "ios": "cd ios && xcodebuild clean && cd ../ && react-native run-ios",
    "clean:android": "cd android && ./gradlew clean && cd ../",
    "clean:ios": "cd ios && xcodebuild clean && cd ../",
    "aab": "npx jetify && cd android && ./gradlew clean && ./gradlew bundleRelease && cd ../",
    "apk": "npx jetify && cd android && ./gradlew clean && ./gradlew assembleStaging && cd ../",
    "genimg": "node genimg src/assets/images --output src/res/images.ts --name images",
    "postinstall": "npx patch-package && npx typesync && RCT_NEW_ARCH_ENABLED=0  npx pod-install ios",
    "typesync": "npx typesync"
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "formik": "^2.4.5",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-native": "0.73.1",
    "react-native-flash-message": "^0.4.2",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-iphone-x-helper": "^1.3.1",
    "react-native-reanimated": "^3.6.1",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.29.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
